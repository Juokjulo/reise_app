<p id="notice"><%= notice %></p>

<h1>Travel Tipps Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @travel_tipps_categories.each do |travel_tipps_category| %>
      <tr>
        <td><%= travel_tipps_category.name %></td>
        <td><%= link_to 'Show', travel_tipps_category %></td>
        <td><%= link_to 'Edit', edit_travel_tipps_category_path(travel_tipps_category) %></td>
        <td><%= link_to 'Destroy', travel_tipps_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Travel Tipps Category', new_travel_tipps_category_path %>
